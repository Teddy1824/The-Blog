{"ast":null,"code":"import \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/es.json.stringify.js\";\nexport default {\n  name: \"SignIn\",\n  data: function data() {\n    return {\n      email: \"\",\n      password: \"\"\n    };\n  },\n  methods: {\n    signin: function signin() {\n      var _this = this;\n\n      fetch(\"https://the-anything-blog1.herokuapp.com/users/signin\", {\n        method: \"POST\",\n        body: JSON.stringify({\n          email: this.email,\n          password: this.password\n        }),\n        headers: {\n          \"Content-type\": \"application/json; charset=UTF-8\"\n        }\n      }).then(function (res) {\n        return res.json();\n      }).then(function (json) {\n        alert(\"User logged in\");\n        localStorage.setItem(\"access\", json.access);\n\n        _this.$router.push({\n          name: \"home\"\n        });\n      })[\"catch\"](function (err) {\n        alert(err);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";;AA8BA,eAAe;EACbA,IAAI,EAAE,QADO;EAEbC,IAFa,kBAEN;IACL,OAAO;MACLC,KAAK,EAAE,EADF;MAELC,QAAQ,EAAE;IAFL,CAAP;EAID,CAPY;EASbC,OAAO,EAAE;IACPC,MADO,oBACE;MAAA;;MACPC,KAAK,CAAC,uDAAD,EAA0D;QAC7DC,MAAM,EAAE,MADqD;QAE7DC,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAe;UACnBR,KAAK,EAAE,KAAKA,KADO;UAEnBC,QAAQ,EAAE,KAAKA;QAFI,CAAf,CAFuD;QAM7DQ,OAAO,EAAE;UACP,gBAAgB;QADT;MANoD,CAA1D,CAAL,CAUGC,IAVH,CAUQ,UAACC,GAAD;QAAA,OAASA,GAAG,CAACC,IAAJ,EAAT;MAAA,CAVR,EAWGF,IAXH,CAWQ,UAACE,IAAD,EAAU;QACdC,KAAK,CAAC,gBAAD,CAAL;QACAC,YAAY,CAACC,OAAb,CAAqB,QAArB,EAA+BH,IAAI,CAACI,MAApC;;QACA,KAAI,CAACC,OAAL,CAAaC,IAAb,CAAkB;UAAEpB,IAAI,EAAE;QAAR,CAAlB;MACD,CAfH,WAgBS,UAACqB,GAAD,EAAS;QACdN,KAAK,CAACM,GAAD,CAAL;MACD,CAlBH;IAmBD;EArBM;AATI,CAAf","names":["name","data","email","password","methods","signin","fetch","method","body","JSON","stringify","headers","then","res","json","alert","localStorage","setItem","access","$router","push","err"],"sourceRoot":"","sources":["C:\\Users\\LC Studio Intern 33\\Documents\\Project\\The-Blog-Frontend\\src\\views\\SignIn.vue"],"sourcesContent":["<template>\r\n  <section class=\"signup\">\r\n    <form @submit.prevent=\"signin()\" class=\"signup-form\">\r\n      <h2>Welcome Back!</h2>\r\n      <div class=\"form-control\">\r\n        <label for=\"Email\">Email</label>\r\n        <input type=\"email\" v-model=\"email\" name=\"email\" />\r\n      </div>\r\n      <div class=\"form-control\">\r\n        <label for=\"Password\">Password</label>\r\n        <input type=\"password\" v-model=\"password\" name=\"password\" />\r\n      </div>\r\n      <div class=\"form-button\">\r\n        <button type=\"submit\">Submit</button>\r\n      </div>\r\n      <div class=\"form-redirect\">\r\n        <h4>\r\n          Don't have an account?\r\n          <span\r\n            ><router-link :to=\"{ path: '/signup' }\" class=\"sign-up\"\r\n              >Click here to sign up!</router-link\r\n            ></span\r\n          >\r\n        </h4>\r\n      </div>\r\n    </form>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"SignIn\",\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    signin() {\r\n      fetch(\"https://the-anything-blog1.herokuapp.com/users/signin\", {\r\n        method: \"POST\",\r\n        body: JSON.stringify({\r\n          email: this.email,\r\n          password: this.password,\r\n        }),\r\n        headers: {\r\n          \"Content-type\": \"application/json; charset=UTF-8\",\r\n        },\r\n      })\r\n        .then((res) => res.json())\r\n        .then((json) => {\r\n          alert(\"User logged in\");\r\n          localStorage.setItem(\"access\", json.access);\r\n          this.$router.push({ name: \"home\" });\r\n        })\r\n        .catch((err) => {\r\n          alert(err);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.signup {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  width: 100%;\r\n  min-height: 100vh;\r\n  background-color: #7ed6df;\r\n  background-image: url(\"../assets/signin.svg\");\r\n  background-repeat: no-repeat;\r\n  background-size: 450px;\r\n  background-position: bottom right;\r\n}\r\nh2 {\r\n  color: white;\r\n  margin: 0;\r\n}\r\n.signup-form {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  background-color: #130f40;\r\n  gap: 1rem;\r\n  width: 400px;\r\n  padding: 30px 15px;\r\n  border-radius: 3px;\r\n  box-shadow: 0px 0px 15px #6ed4d2;\r\n}\r\n.form-control {\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: flex-start;\r\n  flex-direction: column;\r\n  gap: 1rem;\r\n}\r\n.form-control input,\r\nlabel {\r\n  width: 98%;\r\n}\r\n.form-control label {\r\n  font-size: 12px;\r\n  font-weight: 600;\r\n  color: white;\r\n  margin: 0;\r\n}\r\n.form-control input {\r\n  height: 30px;\r\n  border: none;\r\n  outline: none;\r\n  border-radius: 3px;\r\n  font-size: 12px;\r\n}\r\n.form-button {\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  gap: 1rem;\r\n}\r\n.form-redirect {\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  gap: 1rem;\r\n}\r\nbutton {\r\n  min-width: 100px;\r\n  padding: 8px 10px;\r\n  border: none;\r\n  border-radius: 3px;\r\n  font-weight: 600;\r\n  font-size: 10px;\r\n  background-color: #7ed6df;\r\n  color: #130f40;\r\n}\r\nh4 {\r\n  font-weight: 600;\r\n  font-size: 10px;\r\n  color: white;\r\n}\r\n.sign-up {\r\n  color: #7ed6df;\r\n  text-decoration: none;\r\n}\r\n@media screen and (max-width: 600px) {\r\n  .signup {\r\n    background-image: none;\r\n  }\r\n  .signup-form {\r\n    max-width: 85%;\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}