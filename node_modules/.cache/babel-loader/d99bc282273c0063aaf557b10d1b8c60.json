bel for=\"subtitle\" class=\"form-label\">Subtitle</label>\r\n        <input\r\n          class=\"form-control\"\r\n          type=\"text\"\r\n          name=\"subtitle\"\r\n          id=\"subtitle\"\r\n          v-model=\"subtitle\"\r\n        />\r\n      </div>\r\n      <div class=\"mb-3\">\r\n        <label for=\"category\" class=\"form-label\">Category</label>\r\n        <select\r\n          class=\"form-select\"\r\n          name=\"category\"\r\n          id=\"category\"\r\n          v-model=\"category\"\r\n        >\r\n          <option value=\"Tech\">Tech</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"mb-3\">\r\n        <label for=\"image\" class=\"form-label\">Image URL</label>\r\n        <input\r\n          class=\"form-control\"\r\n          type=\"text\"\r\n          name=\"image\"\r\n          id=\"image\"\r\n          v-model=\"main_image\"\r\n        />\r\n      </div>\r\n      <div class=\"mb-3\">\r\n        <label for=\"desc\" class=\"form-label\">Description</label>\r\n        <input\r\n          class=\"form-control\"\r\n          type=\"text\"\r\n          name=\"desc\"\r\n          id=\"desc\"\r\n          v-model=\"desc\"\r\n        />\r\n      </div>\r\n    </MDBModalBody>\r\n    <MDBModalFooter>\r\n      <MDBBtn color=\"secondary\" @click=\"exampleModal2 = false\">Close</MDBBtn>\r\n      <MDBBtn color=\"primary\" @click=\"createBlog()\">Create</MDBBtn>\r\n    </MDBModalFooter>\r\n  </MDBModal>\r\n  <div class=\"home-banner\">\r\n    <div class=\"mobile-banner-menu\">\r\n      <div class=\"menu\">\r\n        <div class=\"bar\"></div>\r\n      </div>\r\n    </div>\r\n    <div class=\"banner-search\">\r\n      <h2>Search</h2>\r\n      <input\r\n        type=\"text\"\r\n        name=\"search\"\r\n        v-model=\"search\"\r\n        placeholder=\"Search posts\"\r\n      />\r\n    </div>\r\n\r\n    <div class=\"banner-search make-button\">\r\n      <button\r\n        color=\"black\"\r\n        aria-controls=\"exampleModal2\"\r\n        @click=\"exampleModal2 = true\"\r\n      >\r\n        Create Blog\r\n      </button>\r\n    </div>\r\n  </div>\r\n  <div class=\"container flex\">\r\n    <div v-if=\"tech.length\">\r\n      <div\r\n        v-for=\"techs in filtertech\"\r\n        :key=\"techs._id\"\r\n        class=\"grid grid-cols-3 grid-rows-3 md:grid-cols-6 sm:grid-cols-12 grid-flow-row justify-center\"\r\n      >\r\n    \r\n<div class=\"container\">\r\n        </div>\r\n        <div class=\"card\">\r\n    <div class=\"img-container\" :style=\"{ background: `url(${tech.main_image})`, backgroundSize: 'cover', backgroundRepeat: 'no-repeat'}\"></div>\r\n    <div class=\"card-content\">\r\n      <h1>{{ tech.title }}</h1>\r\n      <h2>{{tech.subtitle}}</h2>\r\n      <p class=\"excerpt\">{{tech.desc}}</p>\r\n      <p class=\"author\"> <img\r\n                  :src=\"tech.user_image\"\r\n                  alt=\"user__image\"\r\n                  class=\"user__image\"\r\n                />{{tech.created_by}}\r\n              <router-link\r\n              :to=\"{\r\n                name: 'musicdetails',\r\n                params: {\r\n                  id: musics._id,\r\n                },\r\n              }\"\r\n            >\r\n              <div class=\"make-button\">\r\n                <button>View Blog</button>\r\n              </div>\r\n            </router-link>\r\n      </p>\r\n \r\n    </div>\r\n  </div>\r\n\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else>\r\n      <p class=\"m-5 center\">Loading tech...</p>\r\n      <div class=\"orbit-spinner\">\r\n        <div class=\"orbit\"></div>\r\n        <div class=\"orbit\"></div>\r\n        <div class=\"orbit\"></div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  MDBModal,\r\n  MDBModalHeader,\r\n  MDBModalTitle,\r\n  MDBModalBody,\r\n  MDBModalFooter,\r\n  MDBBtn,\r\n} from \"mdb-vue-ui-kit\";\r\nimport { ref } from \"vue\";\r\nexport default {\r\n  components: {\r\n    MDBModal,\r\n    MDBModalHeader,\r\n    MDBModalTitle,\r\n    MDBModalBody,\r\n    MDBModalFooter,\r\n    MDBBtn,\r\n  },\r\n  data() {\r\n    return {\r\n      tech: [],\r\n      title: \"\",\r\n      subtitle: \"\",\r\n      desc: \"\",\r\n      category: \"\",\r\n      main_image: \"\",\r\n      search: \"\",\r\n      exampleModal2: \"\",\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    fetch(\"https://the-anything-blog1.herokuapp.com/tech\", {\r\n      method: \"GET\",\r\n      headers: {\r\n        \"Content-type\": \"application/json; charset=UTF-8\",\r\n      },\r\n    })\r\n      .then((response) => response.json())\r\n      .then((json) => {\r\n        this.tech = json;\r\n      });\r\n  },\r\n\r\n  setup() {\r\n    const exampleModal2 = ref(false);\r\n    return {\r\n      exampleModal2,\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    filtertech() {\r\n      return this.tech.filter((techs) => {\r\n        return techs.title.toLowerCase().match(this.search.toLowerCase());\r\n      });\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    // Create Product\r\n    createBlog() {\r\n      if (!localStorage.getItem(\"access\")) {\r\n        alert(\"User not logged in\");\r\n        return this.$router.push({ name: \"signin\" });\r\n      }\r\n      fetch(\"https://the-anything-blog1.herokuapp.com/tech/technology\", {\r\n        method: \"POST\",\r\n        body: JSON.stringify({\r\n          main_image: this.main_image,\r\n          title: this.title,\r\n          subtitle: this.subtitle,\r\n          desc: this.desc,\r\n          category: this.category,\r\n        }),\r\n        headers: {\r\n          \"Content-type\": \"application/json; charset=UTF-8\",\r\n          Authorization: `Bearer ${localStorage.getItem(\"access\")}`,\r\n        },\r\n      })\r\n        .then((res) => res.json())\r\n        .then((json) => {\r\n          alert(\"Blog created!\");\r\n          location.reload();\r\n        })\r\n        .catch((err) => {\r\n          alert(err);\r\n        });\r\n    },\r\n  },\r\n};\